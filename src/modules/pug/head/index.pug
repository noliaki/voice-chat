head(prefix='og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# mixi: http://mixi-platform.com/ns#', itemscope itemtype="http://schema.org/Article")
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible', content='IE=edge')
  meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')

  meta(name='description', content=DESCRIPTION)
  meta(name='format-detection', content='telephone=no,address=no,email=no')

  //-================ SNS ================-//
  //- ogp
  include ./head_meta_ogp

  //- facebook
  meta(property='fb:app_id', content = 111)

  //- twitter
  include ./head_meta_twitter

  link(itemprop="image", href="")

  //-================ FAVICON ================-//
  link(rel='icon', href = '/favicon.ico')

  //-================ STYLE ================-//
  link(rel='stylesheet', href='css/style.css')
  block style

  //-================ TITLE ================-//
  title= TITLE
